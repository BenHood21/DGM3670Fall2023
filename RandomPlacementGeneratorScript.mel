global proc Cube()
{CreatePolygonCube;
string $components[] = `ls -selection -flatten`;
vector $sphrandValue = `sphrand(5)`;
move -relative ($sphrandValue.x) ($sphrandValue.y) ($sphrandValue.z) $components;
}
global proc Sphere()
{CreatePolygonSphere;
string $components[] = `ls -selection -flatten`;
vector $sphrandValue = `sphrand(5)`;
move -relative ($sphrandValue.x) ($sphrandValue.y) ($sphrandValue.z) $components;
}global proc Plane()
{CreatePolygonPlane;
